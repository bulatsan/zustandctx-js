{
  "name": "@bulatlib/zustandctx",
  "author": "bulatsan",
  "version": "0.0.2",
  "description": "Zustand context helper for TypeScript",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.es.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "zustand",
    "context"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bulatsan/zustandctx-js.git"
  },
  "scripts": {
    "test": "vitest run",
    "build": "tsc -b && vite build -c vite.config.ts",
    "b": "tsc -b && vite build -c vite.config.ts",
    "pack": "rm -rf pack && bun pm cache rm && bun run build && mkdir -p pack && npm pack --pack-destination ./pack",
    "fmt": "prettier --write ."
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@tsconfig/strictest": "^2.0.6",
    "@types/node": "^24.9.1",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^19.2.2",
    "jsdom": "^27.0.1",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.9.3",
    "vite": "^7.1.11",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4",
    "zustand": "^5.0.3"
  }
}
